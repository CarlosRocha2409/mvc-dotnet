    <UserSecretsId>9ada2285-6ff7-4c6b-9b45-01a9ff3ee5ab</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.0" />
